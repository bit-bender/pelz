##############################################################################
#       X509 configuration for experimental pelz "requestor" certificate        #
##############################################################################

[req]
md                      = sha512
prompt                  = no
distinguished_name      = dn
req_extensions          = v3_ext

[dn]
C                       = "US"
O                       = "pelz"
CN                      = "TestPelzRequestor"

[ v3_ext ]
subjectAltName          = @alt_names

[ alt_names ]
IP.0                    = 127.0.0.1
DNS.0                   = localhost.pelzRequestor
